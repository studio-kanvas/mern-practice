{
  "name": "reactjs-starter-seed",
  "version": "0.0.1",
  "description": "ReactJs Starter Seed",
  "scripts": {
    "prestart": "babel-node scripts/startMessage.js",
    "start": "npm-run-all --parallel security-check lint:watch test:watch startServerDev",
    "startServerDev": "babel-node scripts/serverDev.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "mocha --reporter progress --compilers scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "yarn test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/serverProd.js"
  },
  "main": "index.js",
  "repository": "https://github.com/ariesmcrae/reactjs-crud-starter-kit.git",
  "author": "Aries McRae <github@ariesmcrae.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.11.0",
    "json-server": "^0.9.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "open": "^0.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-md5-hash": "^0.0.5"
  }
}
