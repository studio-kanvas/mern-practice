{
  "name": "reactjs-starter-seed",
  "version": "0.0.1",
  "description": "ReactJs Starter Seed",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check lint:watch test:watch start-mockapi startServerDev",
    "startServerDev": "babel-node buildScripts/serverDev.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "mocha --reporter progress --compilers buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "yarn test -- --watch",
    "generate-mock-data": "babel-node buildScripts/mockDataGenerator",
    "prestart-mockapi": "yarn generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/serverProd.js"
  },
  "main": "index.js",
  "repository": "https://bitbucket.org/ariesmcrae/reactjs-starter-seed.git",
  "author": "Aries McRae <aries@ariesmcrae.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "react": "^15.4.2",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.9.1",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "open": "^0.0.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
